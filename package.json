{
	"name": "button-shy-play-bot",
	"main": "dist/main.js",
	"scripts": {
		"build": "tsc",
		"lint": "eslint \"src/**/*.ts\"",
		"prettier": "prettier --write \"src/**/*.ts\"",
		"start": "cross-env NODE_ENV=development tsc-watch --preserveWatchOutput --onSuccess \"node ./dist/main.js\"",
